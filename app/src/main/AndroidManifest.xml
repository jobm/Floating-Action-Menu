<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.job.floatingactionmenu" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".FloatingMenuActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewOne"
            android:label="@string/title_activity_view_one" >
        </activity>
        <activity
            android:name=".ViewTwo"
            android:label="@string/title_activity_view_two" >
        </activity>
        <activity
            android:name=".NelsonPopPoint"
            android:label="@string/title_activity_nelson_pop_point"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".JobCircularImage"
            android:label="@string/title_activity_job_circular_image"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
